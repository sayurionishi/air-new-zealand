});

import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('https://flightbookings.airnewzealand.co.nz/vbook/actions/search');
  await page.getByText('Leave on date, in day day forward slash month month format Please fill out this').click();
  await page.getByRole('option', { name: 'Wellington' }).first().click();
  await page.getByLabel('To airport or city').click();
  await page.getByRole('option', { name: 'Wellington' }).first().click();
  await page.getByLabel('To airport or city').click();
  await page.getByRole('option', { name: 'Christchurch' }).first().click();
  await page.getByLabel('Leave on date, in day day').click();
  await page.getByLabel('Leave on date, in day day').click();
  await page.getByLabel('Leave on date, in day day').fill('01/06');
  await page.getByLabel('Leave on date, in day day').press('Tab');
  await page.getByLabel('Return on date, in day day').fill('30/06');
  await page.locator('#main-container').click();
  await page.getByRole('button', { name: 'Search' }).click();
  await page.locator('#viewpoint-DOMESTIC-2 > div:nth-child(3) > fieldset > div > .vui-si-leg-option-row-wrapper > .vui-si-leg-option-row > div > .vui-si-leg-option-costs-row > div > .vui-si-cost-label > .vui-si-cost-label-value > div:nth-child(2)').first().click();
  await page.locator('div:nth-child(15) > .vui-si-leg-option-row-wrapper > .vui-si-leg-option-row > div > .vui-si-leg-option-costs-row > div:nth-child(2) > .vui-si-cost-label > .vui-si-cost-label-value > div:nth-child(2)').click();
  await page.getByRole('button', { name: 'Continue' }).click();
  await page.getByRole('heading', { name: 'Enter passenger details' }).click();
  await page.getByLabel('Title').selectOption('MR');
  await
});






import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('https://flightbookings.airnewzealand.co.nz/vbook/actions/search');
  await page.getByLabel('From airport or city').click();
  await page.locator('#lwvbcezj').click();
  await page.getByLabel('To airport or city').click();
  await page.locator('#lwvbcfad').click();
  await page.getByLabel('Leave on date, in day day').click();
  await page.getByLabel('Leave on date, in day day').fill('10/06');
  await page.getByLabel('Leave on date, in day day').press('Tab');
  await page.getByLabel('Return on date, in day day').fill('20/06');
  await page.locator('#main-container').click();
  await page.getByRole('button', { name: 'Search' }).click();
  await page.getByRole('button', { name: 'Continue' }).click();
  await page.getByLabel('First name').click();
  await page.getByLabel('First name').fill('Drone');
  await page.getByLabel('First name').press('Tab');
  await page.getByLabel('Family name').fill('Deploy');
  await page.getByLabel('Family name').press('Tab');
  await page.getByLabel('Frequent flyer programme').press('Tab');
  await page.getByLabel('Membership number').press('Tab');
  await page.getByLabel('Country Code').press('Tab');
  await page.getByLabel('Mobile or Landline').fill('123456');
  await page.getByLabel('Mobile or Landline').press('Tab');
  await page.getByLabel('Email address').fill('d@d.co');
  await page.getByLabel('Email address').press('Tab');
  await page.getByLabel('Allow Air New Zealand to tell').press('Tab');
  await page.getByRole('button', { name: 'Continue' }).click();
  await page.getByRole('button', { name: 'Continue' }).click();
  await page.getByRole('heading', { name: 'Select your seats' }).click();
  await page.getByText('13A Occupied Standard economy').click();
  await page.getByText('13B Occupied Standard economy').click();
  await page.getByText('18A Occupied Standard economy').click();
  await page.getByRole('button', { name: 'Available Standard economy seat 21A. $' }).click();
  await page.getByRole('option', { name: 'Drone Deploy Standard economy' }).click();
  await page.getByRole('button', { name: 'Available Standard economy seat 21A. юдв Selected' }).click();
  await page.getByRole('option', { name: 'Drone Deploy Not selected' }).click();
  await page.getByText('27A', { exact: true }).click();
  await page.getByText('27A Unavailable seat 27A.').click();
  await page.getByText('27A Unavailable seat 27A.').click();
  await page.getByText('27A Unavailable seat 27A.').click();
  await page.getByText('28A Unavailable seat 28A.').click();
  await page.getByRole('option', { name: 'Drone Deploy Not selected' }).click();
  await page.getByRole('button', { name: 'Available Standard economy seat 26A. $' }).click();
  await page.getByLabel('Seat information').getByRole('button', { name: 'Next flight' }).click();
  await page.getByRole('option', { name: 'Drone Deploy Not selected' }).click();
  await page.getByRole('button', { name: 'Available Standard economy seat 17A. $' }).click();
  await page.getByLabel('Flight 2 Wellington to').getByRole('button', { name: 'Continue' }).click();
  await page.getByRole('heading', { name: 'Review and pay' }).click();
  await page.getByRole('option', { name: 'Drone Deploy Standard economy' }).click();
  await page.getByRole('link', { name: 'Select your flights' }).click();
  await page.getByText('Enter passenger details').click();
  await page.getByText('Wellington to Auckland').click();
  await page.getByText('Auckland to Wellington').click();
  await page.locator('vui-selectitinerary').click();
});