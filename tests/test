  await page.getByLabel('From airport or city').click();
  await page.getByRole('option', { name: 'Angeles/Mabalacat' }).click();
  await page.getByLabel('From airport or city').click();
  await page.getByRole('option', { name: 'Charleville' }).click();
  await page.getByLabel('To airport or city').click();
  await page.getByRole('option', { name: 'Aberdeen' }).click();
  await page.getByLabel('From airport or city').click();
  await page.getByRole('option', { name: 'Aberdeen' }).click();
  await page.getByLabel('To airport or city').click();
  await page.getByRole('option', { name: 'Abu Dhabi' }).click();
