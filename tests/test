  await page.locator('#calendarpanel-0').getByText('15').click();
  await page.locator('#calendarpanel-0').getByText('1', { exact: true }).nth(1).click();
  await page.getByLabel('Return on date, in day day').click();
  await page.locator('#calendarpanel-1').getByText('7', { exact: true }).click();
  await page.getByRole('button', { name: 'Search' }).click();
  await page.frameLocator('iframe[name="a-2agm1zh9w9ll"]').getByLabel('I\'m not a robot').click();
  await page.frameLocator('iframe[name="c-2agm1zh9w9ll"]').locator('tr:nth-child(3) > td:nth-child(2)').click();
  await page.frameLocator('iframe[name="c-2agm1zh9w9ll"]').locator('tr:nth-child(3) > td:nth-child(3)').click();
  await page.frameLocator('iframe[name="c-2agm1zh9w9ll"]').locator('tr:nth-child(2) > td').first().click();
  await page.frameLocator('iframe[name="c-2agm1zh9w9ll"]').locator('tr:nth-child(3) > td').first().click();
  await page.frameLocator('iframe[name="c-2agm1zh9w9ll"]').getByRole('button', { name: 'Verify' }).click();
  await page.getByRole('button', { name: 'Submit' }).click();
